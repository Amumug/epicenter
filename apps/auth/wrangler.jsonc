{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "auth",
	"main": "src/index.ts",
	"compatibility_date": "2025-07-24",
	"env": {
		"development": {
			"vars": {
				"ENVIRONMENT": "development"
			},
			"observability": {
				"enabled": true,
				"head_sampling_rate": 1
			}
		},
		"production": {
			"vars": {
				"ENVIRONMENT": "production"
			},
			"observability": {
				"enabled": true,
				"head_sampling_rate": 1
			}
		}
	}
}
